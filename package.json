{
	"name": "eleventy-plugin-vento",
	"version": "5.2.0",
	"description": "Adds support for the Vento templating language to Eleventy.",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"test": "vitest --run",
		"lint": "pnpm --color run '/^lint:/'",
		"lint:syntax": "eslint '.'",
		"lint:types": "tsc --noEmit",
		"lint:project": "knip",
		"build": "tsdown",
		"dev": "tsdown --watch ./src",
		"ci:version": "changeset version",
		"ci:publish": "npm audit signatures && changeset publish",
		"fmt": "prettier --write ."
	},
	"types": "./dist/plugin.d.ts",
	"exports": {
		".": {
			"import": "./dist/plugin.js",
			"types": "./dist/plugin.d.ts"
		}
	},
	"files": [
		"dist/*",
		"LICENSE",
		"CHANGELOG.md",
		"readme.md"
	],
	"repository": "git@github.com:noelforte/eleventy-plugin-vento.git",
	"bugs": {
		"url": "https://github.com/noelforte/eleventy-plugin-vento/issues"
	},
	"keywords": [
		"eleventy",
		"eleventy-plugin",
		"ventojs",
		"templating"
	],
	"author": {
		"name": "Noel Forte",
		"email": "noel@forte.is",
		"url": "https://forte.is/"
	},
	"license": "MIT",
	"dependencies": {
		"debug": "4.4.3",
		"ventojs": "2.2.0"
	},
	"devDependencies": {
		"11ty.ts": "0.0.6",
		"@11ty/eleventy": "3.1.2",
		"@changesets/cli": "2.29.7",
		"@eslint/js": "9.37.0",
		"@types/debug": "4.1.12",
		"@types/node": "22.18.10",
		"eslint": "9.37.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-unicorn": "61.0.2",
		"globals": "16.4.0",
		"knip": "5.65.0",
		"prettier": "3.6.2",
		"tsdown": "0.15.9",
		"type-fest": "5.1.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.46.1",
		"vitest": "3.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"oxc-resolver"
		]
	}
}
