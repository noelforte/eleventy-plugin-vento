{
	"name": "eleventy-plugin-vento",
	"version": "1.1.0",
	"description": "Adds support for the Vento templating language to Eleventy.",
	"type": "module",
	"packageManager": "pnpm@9.1.4",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "rm -rf test/output; cd test/input; eleventy",
		"test-serve": "rm -rf test/output; cd test/input; eleventy --serve"
	},
	"main": "./VentoPlugin.js",
	"repository": "git@github.com:noelforte/eleventy-plugin-vento.git",
	"bugs": {
		"url": "https://github.com/noelforte/eleventy-plugin-vento/issues"
	},
	"keywords": [
		"eleventy",
		"eleventy-plugin",
		"ventojs",
		"templating"
	],
	"author": {
		"name": "Noel Forte",
		"email": "noel@forte.is",
		"url": "https://forte.is/"
	},
	"license": "MIT",
	"dependencies": {
		"ventojs": "0.12.6"
	},
	"devDependencies": {
		"eslint": "9.4.0",
		"eslint-config-flat-gitignore": "0.1.5",
		"eslint-config-prettier": "9.1.0",
		"globals": "15.3.0",
		"@11ty/eleventy": "3.0.0-alpha.10"
	},
	"prettier": {
		"bracketSameLine": true,
		"singleQuote": true,
		"semi": true,
		"trailingComma": "es5"
	}
}
