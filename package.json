{
	"name": "eleventy-plugin-vento",
	"version": "5.2.0",
	"description": "Adds support for the Vento templating language to Eleventy.",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"dev": "tsdown --watch ./src",
		"fmt": "prettier --write ."
	},
	"types": "./dist/plugin.d.ts",
	"exports": {
		".": {
			"import": "./dist/plugin.js",
			"types": "./dist/plugin.d.ts"
		}
	},
	"files": [
		"dist/*",
		"LICENSE",
		"CHANGELOG.md",
		"readme.md"
	],
	"repository": "git@github.com:noelforte/eleventy-plugin-vento.git",
	"bugs": {
		"url": "https://github.com/noelforte/eleventy-plugin-vento/issues"
	},
	"keywords": [
		"eleventy",
		"eleventy-plugin",
		"ventojs",
		"templating"
	],
	"author": {
		"name": "Noel Forte",
		"email": "noel@forte.is",
		"url": "https://forte.is/"
	},
	"license": "MIT",
	"dependencies": {
		"debug": "4.4.3",
		"ventojs": "2.2.0"
	},
	"devDependencies": {
		"11ty.ts": "0.0.6",
		"@11ty/eleventy": "3.1.2",
		"@changesets/cli": "2.29.7",
		"@eslint/js": "9.39.1",
		"@types/debug": "4.1.12",
		"@types/node": "24.10.1",
		"eslint": "9.39.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-unicorn": "62.0.0",
		"globals": "16.5.0",
		"knip": "5.70.1",
		"prettier": "3.6.2",
		"tsdown": "0.16.5",
		"type-fest": "5.2.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.47.0",
		"vitest": "4.0.10"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"oxc-resolver"
		],
		"overrides": {
			"js-yaml@<3.14.2": ">=3.14.2",
			"js-yaml@>=4.0.0 <4.1.1": ">=4.1.1"
		}
	}
}
