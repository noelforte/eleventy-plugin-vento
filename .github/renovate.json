{
	"extends": ["github>noelforte/renovate-config"],
	"semanticCommits": "disabled",
	"gitIgnoredAuthors": ["github-actions[bot]@users.noreply.github.com>"],
	"automergeType": "pr",
	"packageRules": [
		{
			"matchDepNames": ["ventojs", "debug"],
			"assignees": ["noelforte"],
			"automerge": false
		}
	],
	"vulnerabilityAlerts": {
		"labels": ["security"],
		"automerge": true,
		"assignees": ["noelforte"],
		"vulnerabilityFixStrategy": "highest"
	},
	"osvVulnerabilityAlerts": true
}
