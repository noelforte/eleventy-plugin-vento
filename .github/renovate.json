{
	"extends": ["github>noelforte/renovate-config", "group:allNonMajor"],
	"semanticCommits": "enabled",
	"customManagers": [
		{
			"customType": "regex",
			"fileMatch": ["(^|\\/)\\.mise\\.toml$"],
			"matchStrings": [
				"#renovate: datasource=(?<datasource>\\S+)(\\s|\\sversioning=(?<versioning>\\S+)\\s)(?<depName>\\S+)\\s?=\\s?\"(?<currentValue>[^\\\"]+)\""
			],
			"versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
		}
	]
}
